<launch>
  <!-- <param name ="use_sim_time" value="true"/> -->
  <param name="robot_description" command="cat $(find vehicle_models)/urdf/civic.urdf" />

  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" output="log">
    <param name="use_tf_static" value="false" />
  </node>

  <node pkg="tf" type="static_transform_publisher" name="link1_broadcaster" args="0 0 0 0 0 0 1 ego_vehicle base_link 50" />
  <!-- <node pkg="tf2_ros" type="static_transform_publisher" name="link1_broadcaster" args="0 0 0 0 0 0 1 ego_vehicle base_link" /> -->
</launch>
